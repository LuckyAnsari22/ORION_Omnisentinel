# Environment Variables Template
# Copy this file to .env and fill in your values

# Frontend Configuration
VITE_APP_NAME=AI Assistant
VITE_APP_VERSION=1.0.0

# AI System URLs (Development)
VITE_GUARDIAN_URL=http://localhost:5001
VITE_VISUALKY_URL=http://localhost:5173

# AI System URLs (Production)
# VITE_GUARDIAN_URL=https://guardian.yourdomain.com
# VITE_VISUALKY_URL=https://visualky.yourdomain.com

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_ANALYTICS=false

# Performance Settings
VITE_TARGET_FPS=60
VITE_ENABLE_SHADOWS=true
VITE_ENABLE_POST_PROCESSING=true

# Debug Mode
VITE_DEBUG_MODE=false
